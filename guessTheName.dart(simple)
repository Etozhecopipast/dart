import "dart:io";

String prompt (String promptText){
  
  print (promptText);
  String answer = stdin.readLineSync()!;
  return answer;
}

double promptDouble(){
  
  print("Enter a number:");
  double myNum = double.parse(stdin.readLineSync()!);
  return myNum;
}

void main() {

    print("Guess the name game");
  
    String answer = 'ilya melnikov';
    String guess = '';
    int gueesCount = 0;

    while(guess != answer){
      guess = prompt("Enter a guess");
      gueesCount++;
    }
    
    print("You won!(Number of attempts ${gueesCount})");
}
